import qrcode

url = input("Enter the URL: ").strip()
file_path = "qrcode.png" # this saves the qrcode to your desktop, for a specific file location, be sure to put the location in front of qrcode.png

qr = qrcode.QRCode()
qr.add_data(url)

img = qr.make_image()
img.save(file_path)

print("QR Code was generated!")
